{
  "name": "@geut/jsdast",
  "version": "0.0.3",
  "description": "Syntax tree JSDoc based on Unist spec",
  "main": "src/index.js",
  "files": [
    "lib",
    "src",
    "bin",
    "index.js"
  ],
  "scripts": {
    "start": "node index.js",
    "test": "jest --passWithNoTests",
    "posttest": "npm run lint",
    "lint": "standard",
    "version": "chan release --allow-yanked ${npm_package_version} && git add .",
    "prepublishOnly": "npm test"
  },
  "dependencies": {
    "fast-glob": "^3.2.5",
    "lodash.trim": "^4.5.1",
    "pascalcase": "^1.0.0",
    "ts-morph": "^9.1.0",
    "typescript": "^4.1.2",
    "unist-builder": "^2.0.3",
    "unist-util-parents": "^1.0.3"
  },
  "devDependencies": {
    "@geut/chan": "^2.0.0",
    "jest": "^24.8.0",
    "standard": "^16.0.3",
    "to-vfile": "^6.1.0",
    "unified": "^9.2.0"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "standard": {
    "env": [
      "jest",
      "node",
      "browser"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geut/jsdast.git"
  },
  "keywords": [
    "jsdoc",
    "tsdoc",
    "unist",
    "ast"
  ],
  "author": {
    "name": "GEUT",
    "email": "contact@geutstudio.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geut/jsdast/issues"
  },
  "homepage": "https://github.com/geut/jsdast#readme",
  "publishConfig": {
    "access": "public"
  }
}
